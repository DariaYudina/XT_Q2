@using Epam.WebPages.UIweb.Models
@{
    Guid userId ;
    Guid awardId ;
    if (IsPost)
    {
        if(Guid.TryParse(Request["userId"], out userId) && Guid.TryParse(Request["awardId"], out awardId))
        {
             UserModel.DeleteAward(userId, awardId);
        }
        Response.Redirect("~/Pages/index.cshtml");
    }   
}
